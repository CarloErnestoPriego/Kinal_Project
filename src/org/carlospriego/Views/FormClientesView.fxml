<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.carlospriego.Controllers.FormClientesController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Formulario%20Clientes.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfClienteId" editable="false" layoutX="56.0" layoutY="171.0" onAction="#handleButtonAction" prefHeight="43.0" prefWidth="347.0" style="-fx-background-radius: 10;">
         <font>
            <Font size="19.0" />
         </font></TextField>
      <TextField fx:id="tfNombre" layoutX="56.0" layoutY="271.0" onAction="#handleButtonAction" prefHeight="43.0" prefWidth="347.0" style="-fx-background-radius: 10;">
         <font>
            <Font size="19.0" />
         </font></TextField>
      <TextField fx:id="tfApellido" layoutX="56.0" layoutY="375.0" onAction="#handleButtonAction" prefHeight="43.0" prefWidth="347.0" style="-fx-background-radius: 10;">
         <font>
            <Font size="19.0" />
         </font></TextField>
      <TextField fx:id="tfTelefono" layoutX="56.0" layoutY="474.0" onAction="#handleButtonAction" prefHeight="43.0" prefWidth="347.0" style="-fx-background-radius: 10;">
