      <GridPane layoutX="415.0" layoutY="577.0" prefHeight="149.0" prefWidth="74.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnGuardar" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="45.0" prefWidth="112.0" text="Guardar" />
            <Button fx:id="btnCancelar" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="45.0" prefWidth="112.0" text="Cancelar" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <TextField fx:id="tfNit" layoutX="56.0" layoutY="668.0" onAction="#handleButtonAction" prefHeight="43.0" prefWidth="347.0" style="-fx-background-radius: 10;">
